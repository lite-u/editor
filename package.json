{
  "name": "@lite-u/editor",
  "version": "0.0.2",
  "description": "vector editor",
  "type": "module",
  "scripts": {
    "publish": "npm publish --access public",
    "dev": "tsc --watch",
    "build": "tsc",
    "build-2": "tsc --emitDeclarationOnly",
    "build-3": "tsup"
  },
  "exports": {
